<p id="notice"><%= notice %></p>

<h1>Days</h1>

<table class="table-striped"  >
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @days.each do |day| %>
      <tr>
        <td><%= link_to day.name, day %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
